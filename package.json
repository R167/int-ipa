{
  "name": "int-ipa",
  "author": "Winston Durand <me@winstondurand.com>",
  "description": "An interactive tool for learning IPA transcription",
  "license": "MIT",
  "version": "0.2.3",
  "repository": "github:R167/int-ipa",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "escape-string-regexp": "^4.0.0",
    "js-base64": "^3.6.0",
    "markdown-to-jsx": "^7.1.0",
    "react": "^17.0.1",
    "react-async-hook": "^3.6.2",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "smoothscroll-polyfill": "^0.4.4",
    "superstruct": "^0.11.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4",
    "yaml": "^1.10.0",
    "zbase32": "^1.0.3"
  },
  "scripts": {
    "preversion": "yarn test --watchAll=false",
    "postversion": "echo \"Pushing $npm_package_version to origin...\" && git push --follow-tags",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "fix": "eslint --fix ./src",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "schema": "ts-json-schema-generator"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:react-hooks/recommended"
    ],
    "rules": {
      "sort-imports": [
        "warn",
        {
          "ignoreDeclarationSort": true
        }
      ],
      "react-hooks/exhaustive-deps": [
        "warn",
        {
          "additionalHooks": "(useAsync|useAsyncCallback)"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      "last 2 year and last 2 major versions and >0.1%",
      ">1%",
      "not dead",
      "not op_mini all",
      "not ie 11 and supports css-sticky"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@types/react-router-dom": "^5.1.6",
    "babel-loader": "^8.1.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^4.5.0",
    "isomorphic-webcrypto": "^2.3.6",
    "path": "^0.12.7",
    "prettier": "^2.2.0",
    "source-map-explorer": "^2.5.0",
    "ts-json-schema-generator": "^0.78.0",
    "webpack": "^4.44",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  }
}
